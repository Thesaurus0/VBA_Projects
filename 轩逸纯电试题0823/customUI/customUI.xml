<?xml version="1.0" encoding="utf-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui" onLoad="ERP_UI_Onload">
	<ribbon startFromScratch="false">
		<tabs>
			<tab id="ERP_2010" label="试题转换工具">
				<group id="grpInputSheets" label="Input">
					<toggleButton id="tbtnShowshtRawData" tag="shtRawData" getLabel="Button_getLabel" getSize="Button_getSize" getImage="Button_getImage"  onAction="ToggleButtonToSwitchSheet_onAction" getPressed="ToggleButtonToSwitchSheet_getPressed" getScreentip="Button_getScreentip"  getSupertip="Button_getSupertip"/>
				</group> 
				<group id="grpMainFunc" label="转换">
					<button id="btnGenReports" getLabel="Button_getLabel" getImage="Button_getImage" getSize="Button_getSize" onAction="Button_onAction" getScreentip="Button_getScreentip"  getSupertip="Button_getSupertip"/>
				</group>
				<group id="grpOutputSheets" label="Output">
					<toggleButton id="tbtnShowshtOutput" tag="shtOutput" getLabel="Button_getLabel" getSize="Button_getSize" getImage="Button_getImage"  onAction="ToggleButtonToSwitchSheet_onAction" getPressed="ToggleButtonToSwitchSheet_getPressed" getScreentip="Button_getScreentip"  getSupertip="Button_getSupertip"/>
					<separator id="spSummAmount02"/>
					<toggleButton id="tbtnShowshtLog" tag="shtLog" getLabel="Button_getLabel" getSize="Button_getSize" getImage="Button_getImage"  onAction="ToggleButtonToSwitchSheet_onAction" getPressed="ToggleButtonToSwitchSheet_getPressed" getScreentip="Button_getScreentip"  getSupertip="Button_getSupertip"/>
				</group> 
				<group id="grpClearData" label="清空数据">
					<button id="btnClearData" getLabel="Button_getLabel" getImage="Button_getImage" getSize="Button_getSize" onAction="Button_onAction" getScreentip="Button_getScreentip"  getSupertip="Button_getSupertip"/>
				</group>
				<group id="grpOthers" label="其他">
					<button id="btnOpenFileFolder" getLabel="Button_getLabel" getImage="Button_getImage" getSize="Button_getSize" onAction="Button_onAction" getScreentip="Button_getScreentip"  getSupertip="Button_getSupertip"/>
				</group> 
			</tab>
		</tabs>
	</ribbon>
</customUI>
